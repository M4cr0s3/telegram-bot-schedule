{
  "version": 2,
  "public": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/index.ts"
    }
  ],
  "redirects": [{ "source": "/", "destination": "/api", "permanent": false }],
  "crons": [
    {
      "path": "/api/cron.ts",
      "schedule": "0 13 * * *"
    }
  ]
}
